{
  "business_units": {
    "large customers": {
      "go_to_market": "direct sales",
      "assumptions": {
        "salesperson_productivity": 1.5,
        "customer_revenue_monthly": 16667
      },
      "formulas": {
        "customers_acquired": "min(salesperson_productivity, 2)",
        "monthly_revenue": "customers_acquired * customer_revenue_monthly"
      }
    },
    "small/medium customers": {
      "go_to_market": "digital marketing",
      "assumptions": {
        "marketing_spend_monthly": 200000,
        "customer_acquisition_cost": 1500,
        "conversion_rate": 0.45,
        "customer_revenue_monthly": 5000
      },
      "formulas": {
        "customers_acquired": "marketing_spend_monthly / customer_acquisition_cost",
        "monthly_revenue": "cumulative_customers * revenue_per_customer",
        "cumulative_customers": "sum(customers_acquired_previous_months)"
      }
    }
  },
  "revenue_drivers": [
    "number of sales people",
    "marketing spend",
    "customer acquisition cost",
    "customer revenue (monthly)",
    "salesperson productivity"
  ]
}
